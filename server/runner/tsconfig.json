{
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES6",
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  },
}
