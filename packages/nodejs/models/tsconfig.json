{
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "outDir": "./lib",
    "target": "ES5",
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
  },
}
